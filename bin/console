#!/usr/bin/env ruby
# frozen_string_literal: true

require "bundler/setup"
require "rbstream"
require "shout"

puts "initializing..."
Shout.shout_init

puts "loaded libshout v#{Shout.shout_version}"

t_shout = Shout.shout_new

puts t_shout

Shout.shout_free(t_shout)

puts "shutting down..."
Shout.shout_shutdown
